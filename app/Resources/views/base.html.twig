<!DOCTYPE html>
<html lang="es">
    <head>
        {% block head %}
        <meta charset="UTF-8" />
        <title>{% block title %}MeetOut{% endblock %}</title>
        {% block stylesheets %}

        {% endblock %}

        <link rel="icon" type="image/x-icon" href="{{ asset('favicon.ico') }}" />

        {% endblock %}
    </head>
    <body>
        {% block body %}
            <header lang="es" class="header-in">
                {% block header %}

                {% endblock %}
            </header>

            <section>
                {% block section %}
                    {% block fos_user_content %}{% endblock %}
                {% endblock %}
            </section>
            <footer>
                {% block footer %}
                {% endblock %}
            </footer>
        {% endblock %}
    </body>
    {% block javascripts %}

    {% endblock %}
</html>
