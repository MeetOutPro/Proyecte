{% extends 'baseout.html.twig' %}
{% block title %}MeetOut - Register {% endblock %}
{% block section %}
    <div class="register">
        <div class="wrapper-register">
                {{ form_start(form, {'method': 'post', 'action': path('registerpage')})}}
                <section class="form-der">
                    {{ form_widget(form.tema) }}
                    <div>
                    {{ form_widget(form.save) }}
                    </div>
                </section>

                <section class="form-izq">
                    <h1>Datos personales</h1>

                    {{ form_widget(form) }}
                    <div id="siguiente-reg">
                       <button type="button">Continuar</button>
                    </div>
                </section>

                {{ form_end(form) }}
        </div>
    </div>
{% endblock %}






